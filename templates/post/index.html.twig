{% extends 'base.html.twig' %}

{% block title %}Hello PostController!
{% endblock %}

{% block body %}


	<div class="container">
		<h1>posts</h1>
		<p>cette page contiendras tous les posts</p>
		{% for article in articles %}
			<div class="card">
				<a href="/{{article.title}}"><h5 class="card-title">{{article.title}}</h5></a>
			<p class="card-text">{{article.article}}</p>
            </div>

		{% endfor %}
	</div>
{% endblock %}
